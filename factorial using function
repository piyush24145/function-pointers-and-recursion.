#include <stdio.h>


int fact(int n) {
    int fact = 1;
    for (int i = 1; i <= n; i++) {
        fact *= i;  // Multiply fact by i in each iteration
    }
    return fact;
}

int main() {
    int number;
    printf("Enter a number: ");
    scanf("%d", &number);
printf("%d",fact(number));
return 0;}
