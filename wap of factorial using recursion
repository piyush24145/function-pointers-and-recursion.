// Online C compiler to run C program online
#include <stdio.h>
int factorial(int n){
    if(n==1)
        return 1;
    
    return n*factorial(n-1);
}


int main() {
    int n;
    printf("enter n :");
    scanf("%d",&n); 
    int fact=factorial(n); // call function (jo chaiye wo likh do)
    printf("%d",fact);    
    return 0;
}
