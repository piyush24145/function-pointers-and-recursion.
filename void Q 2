// Online C compiler to run C program online
#include <stdio.h>
void cricket(){
    printf("you are in ground\n");
    return;
}
void books(){
    printf("you are in library\n");
    cricket();
    return;
}
void hostel(){     // function prototype
    printf("you are in college\n");   
    books();
    return;
}
int main() {
  hostel();     //only calling function 
    return 0;
}
